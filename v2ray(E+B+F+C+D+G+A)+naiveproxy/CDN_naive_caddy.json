//.........<省略>........
              "tls": {
                "sni": ["zv.xx.yy"] //更改为自己规划对应vless+tcp+tls的域名
              }
//.........<省略>........
              "tls": {
                "sni": ["zt.xx.yy"] //更改为自己规划对应trojan+tcp+tls的域名
              }
//.........<省略>........
              "tls": {
                "sni": ["zh.xx.yy"] //更改为自己规划对应https server的域名
              }
//.........<省略>........
            "match": [{
              "host": ["zv.xx.yy","zt.xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的对应域名。
            }],
//.........<省略>........
            "match": [{
              "host": ["zh.xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的对应域名。
            }],
//.........<省略>........


//以上参数修改如下：
//.........<省略>........
              "tls": {
                "sni": ["zv.xx.yy","z1.xx.yy"] //zv.xx.yy更改为自己规划对应vless+tcp+tls的域名，z1.xx.yy更改为自己规划对应vless+ws+tls的CDN应用域名。
              }
//.........<省略>........
              "tls": {
                "sni": ["zt.xx.yy","z2.xx.yy"] //zt.xx.yy更改为自己规划对应trojan+tcp+tls的域名，z2.xx.yy更改为自己规划对应SS+v2ray-plugin+tls的CDN应用域名。
              }
//.........<省略>........
              "tls": {
                "sni": ["zh.xx.yy","z3.xx.yy"] //更改为自己规划对应https server的域名，z3.xx.yy更改为自己规划对应vless+grpc+tls的CDN应用域名。
              }
//.........<省略>........
            "match": [{
              "host": ["zv.xx.yy","zt.xx.yy","z1.xx.yy","z2.xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的对应域名。
            }],
//.........<省略>........
            "match": [{
              "host": ["zh.xx.yy","z3.xx.yy"] //限定域名访问（禁止以ip方式访问网站），更改为自己的对应域名。
            }],
